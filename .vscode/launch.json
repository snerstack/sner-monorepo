    "version": "0.2.0",
    "configurations": [
        {
            "name": "Agent",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "python": "${workspaceFolder}/server/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/server"
            },
            "justMyCode": false,
            "args": ["--assignment", "{\"config\": {\"args\": \"--dummyparam 1\", \"module\": \"auror_hostnames\"}, \"targets\": [\"3\"]}"]
        },
        {
            "name": "Parser",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "python": "${workspaceFolder}/server/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/server"
            },
            "justMyCode": false
        },
        {
            "name": "Python Debugger: Python File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}"
        }
    ]
}